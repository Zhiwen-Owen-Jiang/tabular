{                                                                                                             
  "spark.master": "local[4]",
  "spark.executor.memory": "8g",
  "spark.driver.memory": "8g",
  "spark.executor.cores": "4",
  "spark.num.executors": "1",
  "spark.serializer": "org.apache.spark.serializer.KryoSerializer",
  "spark.kryoserializer.buffer.max": "2048m",
  "spark.shuffle.compress": "true",
  "spark.io.compression.codec": "lz4",
  "spark.default.parallelism": "4",
  "spark.sql.shuffle.partitions": "16",
  "spark.executor.extraJavaOptions": "-XX:ReservedCodeCacheSize=2048m -XX:+PrintCodeCache -XX:+UseCodeCacheFlushing",
  "spark.driver.extraJavaOptions": "-XX:ReservedCodeCacheSize=2048m -XX:+PrintCodeCache -XX:+UseCodeCacheFlushing",
  "spark.sql.codegen.wholeStage": "false",
  "spark.sql.autoBroadcastJoinThreshold": "-1"
}
